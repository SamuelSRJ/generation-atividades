programa {
  funcao inicio() {
    real n1, n2, res
    
    escreva("Digite o primeiro numero: ")
    leia(n1)
    escreva("Digite o segundo numero: ")
    leia(n2)
    res = n1 + n2
    escreva("\nADIÇÃO: ", res)
    res = n1 - n2
    escreva("\nSUBTRAÇÃO: ", res)
    res = n1 * n2
    escreva("\nMULTIPLICAÇÃO: ", res)
    res = n1 / n2
    escreva("\nDIVISÃO: ", res)
  }
}
